#
# GMF AI Audio
#
CONFIG_GMF_AI_AUDIO_INIT_AEC=y
CONFIG_GMF_AI_AUDIO_AEC_FILTER_LEN=4
CONFIG_GMF_AI_AUDIO_AEC_CH_ALLOCATION="RMNM"
# end of GMF AI Audio

#
# GMF Miscellaneous
#
CONFIG_GMF_MISC_INIT_COPIER=y
# end of GMF Miscellaneous
